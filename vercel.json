{
  "version": 2,
  "builds": [
    {
      "src": "build/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["db/db.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
